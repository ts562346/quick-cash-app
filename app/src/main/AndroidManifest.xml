<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quickcashgroup5">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication">
        <activity
            android:name="com.example.quickcashgroup5.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserManagment.LogInActivity"></activity>
        <activity android:name=".UserManagment.LogOutActivity"></activity>
        <activity android:name=".UserManagment.PasswordResetCodeActivity"></activity>
        <activity android:name=".UserManagment.RecoveryAccountActivity"></activity>
        <activity android:name=".UserManagment.ResetPasswordActivity"></activity>
        <activity android:name=".UserManagment.SignUpActivity"></activity>

    </application>

</manifest>